<?xml version="1.0"?>
<dc-workflow workflow_id="gsworkflow" purge="False"
             title="GroupSpace Workflow"
             state_variable="review_state"
             initial_state="private">
 <permission>ims.upload: add chunked file</permission>
 <state state_id="group_published" title="">
  <permission-map name="ims.upload: add chunked file"
                  acquired="False">
   <permission-role>GroupEditor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
 </state>
 <state state_id="private" title="">
  <permission-map name="ims.upload: add chunked file"
                  acquired="False">
   <permission-role>GroupEditor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
 </state>
</dc-workflow>