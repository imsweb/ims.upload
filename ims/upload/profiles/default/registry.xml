<registry>
    <record field="chunksize" interface="ims.upload.interfaces.IChunkSettings"
            name="ims.upload.interfaces.IChunkSettings.chunksize">
        <value>10000000</value>
    </record>
    <record field="hijack" interface="ims.upload.interfaces.IChunkSettings"
            name="ims.upload.interfaces.IChunkSettings.hijack">
        <value>True</value>
    </record>
    <records prefix="plone.resources/blueimp.load-image"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++ims.upload/blueimp/js/load-image.js</value>
    </records>
    <records prefix="plone.resources/blueimp.canvas-to-blob"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++ims.upload/blueimp/js/canvas-to-blob.min.js</value>
    </records>
    <records prefix="plone.resources/blueimp.jquery-iframe-transport"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++ims.upload/blueimp/js/jquery.iframe-transport.js</value>
    </records>
    <records prefix="plone.resources/blueimp.jquery-fileupload"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++ims.upload/blueimp/js/jquery.fileupload.js</value>
    </records>
    <records prefix="plone.resources/blueimp.process"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++ims.upload/blueimp/js/process.js</value>
    </records>
    <records prefix="plone.resources/blueimp.ui"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++ims.upload/blueimp/js/image.js</value>
    </records>
    <records prefix="plone.resources/blueimp.validate"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++ims.upload/blueimp/js/validate.js</value>
    </records>
    <records prefix="plone.resources/blueimp.ui"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++ims.upload/blueimp/js/ui.js</value>
    </records>
    <records prefix="plone.resources/ims.upload"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++ims.upload/upload.js</value>
        <value key="css">
            <element>++resource++ims.upload/upload.css</element>
        </value>
    </records>
    <records prefix="plone.bundles/plone-legacy"
             interface='Products.CMFPlone.interfaces.IBundleRegistry'>
        <value key="merge_with"></value> <!-- important, merging breaks somehow -->
        <value key="resources" purge="false">
            <element>blueimp.load-image</element>
            <element>blueimp.canvas-to-blob</element>
            <element>blueimp.jquery-iframe-transport</element>
            <element>blueimp.jquery-fileupload</element>
            <element>blueimp.process</element>
            <element>blueimp.image</element>
            <element>blueimp.validate</element>
            <element>blueimp.ui</element>
            <element>ims.upload</element>
        </value>
        <value key="depends">plone</value>
        <value key="jscompilation">++plone++static/plone-legacy-compiled.js</value>
        <value key="csscompilation">++plone++static/plone-legacy-compiled.css</value>
        <value key="last_compilation">2017-01-14 00:00:00</value>
        <value key="compile">False</value>
        <value key="enabled">True</value>
    </records>
</registry>